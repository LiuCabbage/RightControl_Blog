@{
    ViewBag.Title = "LiuCabbage-日记";
}
@section css{
    <link href="~/Content/css/timeline.css" rel="stylesheet" />
    <link href="~/Content/css/animate.min.css" rel="stylesheet" />
}
<div class="doc-container" id="doc-container">
    <div class="container-fixed">
        <div class="timeline-box shadow">
            <div class="timeline-main">
                <h1><i class="fa fa-clock-o"></i>日记</h1>
                <div class="timeline-line"></div>
                @Html.DiarysHtml(ViewData["Year"], ViewData["DiarysList"])
                <h1 style="padding-top:4px;padding-bottom:2px;margin-top:40px;"><i class="fa fa-hourglass-end"></i>THE END</h1>
            </div>
        </div>
    </div>
</div>
@section js{
    <script src="~/Content/js/timeline.js"></script>
}