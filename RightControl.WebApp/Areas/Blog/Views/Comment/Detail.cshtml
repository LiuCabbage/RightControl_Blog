@model RightControl.Model.CommentModel
@{
    ViewBag.Title = "Detail";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutDetail.cshtml";
}

<div class="layui-col-md12">
    <table class="layui-table" lay-skin="line">
        <tbody>
            <tr>
                <td>ID：</td>
                <td>@Model.Id</td>
            </tr>
            <tr>
                <td>人员ID：</td>
                <td>@Model.SendId</td>
            </tr>
            <tr>
                <td>目标人员ID：</td>
                <td>@Model.AcceptId</td>
            </tr>
            <tr>
                <td>内容：</td>
                <td>@Model.Content</td>
            </tr>
            <tr>
                <td>状态：</td>
                <td>@Model.Status</td>
            </tr>
            <tr>
                <td>父ID：</td>
                <td>@Model.ParentId</td>
            </tr>
            <tr>
                <td>文章ID：</td>
                <td>@Model.ArticleId</td>
            </tr>
            <tr>
                <td>创建时间：</td>
                <td>@Model.CreateOn</td>
            </tr>
        </tbody>
    </table>
</div>

